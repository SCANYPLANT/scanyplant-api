    "express": "^4.17.1",
    "express-cache-controller": "^1.1.0",
    "express-validator": "^6.2.0",
    "rimraf": "^3.0.0",
    "typeorm": "^0.2.20",
    "uuid": "^3.3.3",
    "pg": "^7.12.1",
    "ts-jest": "^24.2.0",
    "ts-node": "3.3.0",
    "typescript": "3.3.3333",
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
