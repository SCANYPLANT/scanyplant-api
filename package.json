    "eslint-plugin-jest": "^22.20.1",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
